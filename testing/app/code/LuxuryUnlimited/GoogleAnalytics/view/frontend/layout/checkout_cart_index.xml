<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="LuxuryUnlimited\GoogleAnalytics\Block\Cart\UpdateQty" name="ga.cart.update.qty.js"
                   template="LuxuryUnlimited_GoogleAnalytics::js/script.phtml" after="-"/>
            <block class="LuxuryUnlimited\GoogleAnalytics\Block\Cart\RemoveFromCart" name="ga.remove.from.cart.js"
                   after="ga.cart.update.qty.js"
                   template="LuxuryUnlimited_GoogleAnalytics::js/script.phtml"/>
        </referenceContainer>
    </body>
</page>
