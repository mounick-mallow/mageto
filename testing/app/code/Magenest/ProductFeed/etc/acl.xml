<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::marketing">
                    <resource id="Magenest_ProductFeed::feed" title="ProductFeed" sortOrder="60">
                        <resource id="Magenest_ProductFeed::product" title="ProductFeed">
                            <resource id="Magenest_ProductFeed::addedit" title="Add/Edit Feed" sortOrder="10" />
                            <resource id="Magenest_ProductFeed::delete" title="Delete Feed" sortOrder="30" />
                        </resource>
                        <resource id="Magenest_ProductFeed::configuration" title="Config" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magenest_ProductFeed::magenest_config" title="Config ProductFeed"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>