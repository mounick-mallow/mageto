{
    "name": "magento/project-community-edition",
    "description": "eCommerce Platform for Growth (Community Edition)",
    "type": "project",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "magento/magento-composer-installer": true,
            "magento/inventory-composer-installer": true,
            "laminas/laminas-dependency-plugin": true,
            "vaimo/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true,
            "phpstan/extension-installer": true,
            "magento/composer-dependency-version-audit-plugin": true,
            "magento/composer-root-update-plugin": true,
            "php-http/discovery": true
        }
    },
    "version": "2.4.5-p2",
    "require": {
        "adyen/module-payment": "^8.11.1",
        "creativestyle/magesuite-magepack": "^1.1",
        "justbetter/magento2-sentry": "dev-master",
        "liquid/liquid": "1.x-dev",
        "ludxb/magento-meta-package": ">=4.0",
        "magento/composer-dependency-version-audit-plugin": "~0.1",
        "magento/composer-root-update-plugin": "~2.0",
        "magento/product-community-edition": "2.4.6-p3",
        "magento/quality-patches": "^1.1",
        "phpro/mage2-module-cookie-consent": "^3.0",
        "rollbar/rollbar-magento2": "dev-master",
        "rosell-dk/webp-convert": "^2.6",
        "smile/elasticsuite": "~2.11.0",
        "weew/helpers-array": "^1.3.1"
    },
    "require-dev": {
        "allure-framework/allure-phpunit": "~1.5.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "deployer/deployer": "7.*",
        "friendsofphp/php-cs-fixer": "~3.4.0",
        "lusitanian/oauth": "~0.8.10",
        "magento/magento-coding-standard": "*",
        "magento/magento2-functional-testing-framework": "^3.7",
        "mage2tv/magento-cache-clean": "^1.0",
        "pdepend/pdepend": "~2.10.0",
        "phpmd/phpmd": "^2.12.0",
        "phpro/grumphp": "^1.2.0",
        "phpstan/phpstan": "^1.6.8",
        "phpunit/phpunit": "~9.5.20",
        "sebastian/phpcpd": "^6.0.3",
        "squizlabs/php_codesniffer": "~3.6.0",
        "symfony/finder": "^5.2"
    },
    "conflict": {
        "gene/bluefoot": "*"
    },
    "autoload": {
        "psr-4": {
            "Magento\\Framework\\": "lib/internal/Magento/Framework/",
            "Magento\\Setup\\": "setup/src/Magento/Setup/",
            "Magento\\": "app/code/Magento/"
        },
        "psr-0": {
            "": [
                "app/code/",
                "generated/code/"
            ]
        },
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ],
        "exclude-from-classmap": [
            "**/dev/**",
            "**/update/**",
            "**/Test/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\PhpStan\\": "dev/tests/static/framework/Magento/PhpStan/",
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/"
        }
    },
    "minimum-stability": "dev",
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://repo.magento.com/",
            "exclude": ["magento/composer"]
        },
        "phpro/grumphp": {
            "type": "vcs",
            "url": "https://monteshot@bitbucket.org/monteshot/phpro-grumphp.git"
        },
        "ludxb/magento-meta-package": {
            "type": "git",
            "url": "git@github.com:ludxb/magento-meta-package.git"
        }
    },
    "extra": {
        "magento-force": "override",
        "patches": {
        },
        "magento-deploystrategy": "copy",
        "magento-deploy-ignore": {
            "*": [
                "/.github",
                "/.gitignore",
                "/.htaccess",
                "/README.md",
                "/CHANGELOG.md",
                "/SECURITY.md",
                "/LICENSE.txt",
                "/LICENSE_AFL.txt",
                "/nginx.conf.sample",
                "/pub/errors/",
                "/pub/.htaccess"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "([ $COMPOSER_DEV_MODE -eq 0 ] || vendor/bin/phpcs --config-set installed_paths ../../magento/magento-coding-standard/,../../phpcompatibility/php-compatibility/PHPCompatibility)"
        ],
        "post-update-cmd": [
            "([ $COMPOSER_DEV_MODE -eq 0 ] || vendor/bin/phpcs --config-set installed_paths ../../magento/magento-coding-standard/,../../phpcompatibility/php-compatibility/PHPCompatibility)"
        ]
    }
}
